{
	"name": "whc",
	"version": "1.0.0",
	"description": "",
	"main": "build/whc.js",
	"unpkg": "build/whc.umd.js",
	"source": "src/index.js",
	"scripts": {
		"bundle:start": "microbundle watch --compress false -f modern,umd --no-sourcemap --raw=true",
		"bundle:build": "microbundle -f modern,umd --no-sourcemap",
		"clean:dist": "rm -rf dist",
		"clean:build": "rm -rf build",
		"pack:src": "microbundle --compress false -i src/index.js -o dist/ -f modern,umd --no-sourcemap",
		"pack:dist": "microbundle -i src/index.js -o dist/min -f modern,umd --no-sourcemap",
		"pack": "npm-run-all clean:dist pack:*",
		"build": "npm-run-all clean:build bundle:build",
		"start": "npm-run-all clean:build bundle:start"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/excelerondesign/whc.git"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/excelerondesign/whc/issues"
	},
	"homepage": "https://github.com/excelerondesign/whc#readme",
	"devDependencies": {
		"microbundle": "^0.12.1",
		"npm-run-all": "^4.1.5"
	}
}
