{
	"name": "whc",
	"version": "2.0.0",
	"description": "",
	"main": "dist/whc.js",
	"unpkg": "dist/whc.umd.js",
	"source": "src/index.js",
	"mangle": {
		"regex": "^_"
	},
	"scripts": {
		"start": "microbundle watch --compress false -f modern,umd --no-sourcemap --raw=true",
		"pack:src": "microbundle --compress false -f umd,modern --no-sourcemap",
		"pack:dist:umd": "microbundle -i src/index.js -o dist/min/whc.min.js -f umd --no-sourcemap --name WHC",
		"pack:dist:modern": "microbundle -i src/index.js -o dist/min/whc.min.js -f modern --no-sourcemap --name WHC",
		"pack": "run-p pack:src pack:dist:*"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/excelerondesign/whc.git"
	},
	"keywords": [],
	"author": "Frankie (https://github.com/frankie-tech)",
	"contributors": [
		{
			"name": "Frankie",
			"profile": "https://github.com/frankie-tech"
		},
		{
			"name": "Ryan Babka",
			"profile": "https://github.com/4rch1m3d35"
		}
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/excelerondesign/whc/issues"
	},
	"homepage": "https://github.com/excelerondesign/whc#readme",
	"devDependencies": {
		"husky": "^4.2.5",
		"microbundle": "^0.12.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
